{
    "name": "armincms/namayaan",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0", 
        "armincms/alhazen": "~0.1",
        "armincms/snail": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Namayaan\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Namayaan\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
